BEGIN
    POPUP "编辑"
    BEGIN
        MENUITEM "插入(&I)",                      ID_EDIT_INSERT
        MENUITEM "复制(&C)\tCtrl+C",              ID_EDIT_COPY
        MENUITEM "粘贴(&P)\tCtrl+P",              ID_EDIT_PASTE
        MENUITEM "剪切(&T)\tCtrl+X",              ID_EDIT_CUT
        MENUITEM SEPARATOR
        MENUITEM "向左(&L)",                      ID_EDIT_LEFT
        MENUITEM "向右(&R)",                      ID_EDIT_RIGHT
        MENUITEM "向上(&U)",                      ID_EDIT_UP
        MENUITEM "向下(&D)",                      ID_EDIT_DOWN
    END
END

