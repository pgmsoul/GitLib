        MENUITEM "最近的文件",                       ID_FILE_MRU_FILE1, GRAYED
        MENUITEM SEPARATOR
        MENUITEM "退出(&X)",                      ID_APP_EXIT
    END
    POPUP "窗口(&V)"
    BEGIN
        MENUITEM "<占位符>",                       ID_VIEW_TOOLBAR
    END
    POPUP "帮助(&H)"
    BEGIN
        MENUITEM "帮助文档(&D)",                    ID_APP_HELP
        MENUITEM "关于 LayoutEditor(&A)...",      ID_APP_ABOUT
    END
END

